	CONTROL "",                      101, "EDIT", ES_LEFT | ES_AUTOHSCROLL | ES_READONLY | WS_CHILD | WS_VISIBLE | WS_BORDER | WS_TABSTOP, 8, 16, 216, 12
	CONTROL "Available image files", 102, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE, 8, 32, 112, 8
	CONTROL "",                      103, "LISTBOX", LBS_STANDARD | WS_CHILD | WS_VISIBLE | WS_BORDER | WS_VSCROLL | WS_TABSTOP, 8, 40, 216, 136
	CONTROL "Mount",                 IDOK, "BUTTON", BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 8, 176, 40, 14
	CONTROL "Cancel",                IDCANCEL, "BUTTON", BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 184, 176, 40, 14
END


DLG_SHARE DIALOG DISCARDABLE  24, 32, 236, 198
STYLE DS_MODALFRAME | WS_OVERLAPPED | WS_CAPTION | WS_VISIBLE | WS_SYSMENU
CAPTION "Transfer files"
FONT 8, "Helv"
BEGIN
