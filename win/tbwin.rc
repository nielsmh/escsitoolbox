#include "windows.h" 


DLG_TOOLBOX DIALOG DISCARDABLE  20, 40, 420, 94
STYLE DS_MODALFRAME | WS_OVERLAPPED | WS_CAPTION | WS_VISIBLE | WS_SYSMENU | WS_MINIMIZEBOX
CAPTION "Emulated SCSI Toolbox"
FONT 8, "Helv"
BEGIN
	CONTROL "Detected SCSI devices", 100, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE, 8, 7, 183, 8
	CONTROL "",                      101, "LISTBOX", LBS_STANDARD | WS_CHILD | WS_VISIBLE | WS_BORDER | WS_VSCROLL | WS_TABSTOP, 8, 16, 400, 56
	CONTROL "Mount new image",       102, "BUTTON", BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 8, 72, 88, 14
	CONTROL "Transfer files",        103, "BUTTON", BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 104, 72, 88, 14
	CONTROL "Exit",                  IDCANCEL, "BUTTON", BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 328, 72, 80, 14
END


DLG_MOUNT DIALOG DISCARDABLE  24, 32, 236, 198
STYLE DS_MODALFRAME | WS_OVERLAPPED | WS_CAPTION | WS_VISIBLE | WS_SYSMENU
CAPTION "Mount image"
FONT 8, "Helv"
BEGIN
