	CONTROL "",                          101, "EDIT", ES_LEFT | ES_AUTOHSCROLL | ES_READONLY | WS_CHILD | WS_VISIBLE | WS_BORDER | WS_TABSTOP, 8, 16, 216, 12
	CONTROL "Shared directory contents", 102, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE, 8, 32, 112, 8
